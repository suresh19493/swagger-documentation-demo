CREATE TABLE USER_TBL(USERID INT PRIMARY KEY,FIRSTNAME VARCHAR(25) NOT NULL,LASTNAME VARCHAR(25) NOT NULL,PHONENUMBER VARCHAR(10) NOT NULL);
CREATE SEQUENCE  USER_TBL_SEQ INCREMENT 1 START WITH 1000;
   
CREATE SEQUENCE USER_TBL_SEQ START WITH 1000
COMMIT;
select postgres.user_tbl_seq.nextval();

DROP SEQUENCE USER_TBL_SEQ;

select * from user_tbl;

SELECT nextval('USER_TBL_SEQ');

